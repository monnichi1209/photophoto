<p id="notice"><%= notice %></p>

<h1>お問い合わせ内容</h1>

<table>
  <thead>
    <tr>
      <th>お名前</th>
      <th>メール</th>
      <th>お問い合わせ内容</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.name %></td>
        <td><%= contact.email %></td>
        <td><%= contact.content %></td>
        <td><%= link_to '詳細', contact %></td>
        <td><%= link_to '編集', edit_contact_path(contact) %></td>
        <td><%= link_to '削除', contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>

  </tbody>
</table>



<br>
<%= link_to '戻る', pictures_path %>

<%= link_to '新規お問い合わせ', new_contact_path %>
